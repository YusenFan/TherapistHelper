# ===========================================
# Therapist Helper AI - .gitignore
# ===========================================

# ===== SECURITY & ENVIRONMENT =====
# Environment variables and secrets
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# API keys and secrets
secrets/
*.key
*.pem
*.p12
*.pfx

# ===== FRONTEND (Next.js/React) =====
# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# Next.js build output
.next/
out/
build/
dist/

# Next.js cache
.next/cache/

# Vercel deployment
.vercel

# TypeScript
*.tsbuildinfo
next-env.d.ts

# ===== BACKEND (Python/FastAPI) =====
# Python virtual environment
venv/
env/
ENV/
.venv/
.ENV/

# Python cache
__pycache__/
*.py[cod]
*$py.class
*.so
.Python

# Distribution / packaging
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
share/python-wheels/
*.egg-info/
.installed.cfg
*.egg
MANIFEST

# Unit test / coverage reports
htmlcov/
.tox/
.nox/
.coverage
.coverage.*
.cache
nosetests.xml
coverage.xml
*.cover
*.py,cover
.hypothesis/
.pytest_cache/
cover/

# ===== DATABASE =====
# PostgreSQL
*.sql
*.backup
*.dump
*.pgbackup

# SQLite
*.db
*.sqlite
*.sqlite3

# Database migration files (keep structure, ignore data)
migrations/versions/
alembic/versions/

# ===== DEVELOPMENT TOOLS =====
# IDE & Editors
.vscode/
.idea/
*.swp
*.swo
*~

# ===== SYSTEM FILES =====
# macOS
.DS_Store
.AppleDouble
.LSOverride
Icon
._*
.DocumentRevisions-V100
.fseventsd
.Spotlight-V100
.TemporaryItems
.Trashes
.VolumeIcon.icns

# Windows
Thumbs.db
Desktop.ini
$RECYCLE.BIN/
*.lnk

# Linux
*~
.fuse_hidden*
.directory
.Trash-*

# ===== LOGS & TEMPORARY FILES =====
# Log files
*.log
logs/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory
coverage/
*.lcov

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# ===== MEDICAL DATA & HIPAA =====
# CRITICAL: Never commit patient data
patient_data/
client_data/
medical_records/
audio_files/
transcripts/
session_recordings/
*.wav
*.mp3
*.mp4
*.m4a
*.flac
*.aac

# Backup files that might contain PHI
*.bak
*.backup
*.old
*.orig

# ===== DEPLOYMENT & PRODUCTION =====
# Docker
.dockerignore
Dockerfile.prod
docker-compose.prod.yml

# AWS
.aws/
.elasticbeanstalk/

# ===== MISCELLANEOUS =====
# Temporary folders
tmp/
temp/
cache/

# Archive files
*.zip
*.tar.gz
*.rar
*.7z

# Test files
test_files/
mock_data/
sample_data/
